{
  "tasks": {
    "dev": "deno run -A npm:vite",
    "build": "deno run -A npm:vite build",
    "preview": "deno run -A npm:vite preview"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "solid-js",
    "lib": ["dom", "dom.iterable", "esnext"]
  },
  "imports": {
    "solid-js": "npm:solid-js@^1.8.0",
    "solid-js/": "npm:solid-js@^1.8.0/"
  }
}
